{
  "exclude": [
    "node_modules",
    "package"
  ],
  "compilerOptions": {
    "target": "es2020",
    "lib": [
      "es2020"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "module": "commonjs",
    "checkJs": true,
    "allowSyntheticDefaultImports": true,
    "keyofStringsOnly": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "noStrictGenericChecks": true,
    "noEmit": true,
    "suppressExcessPropertyErrors": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "pretty": true,
    "baseUrl": "."
  }
}
